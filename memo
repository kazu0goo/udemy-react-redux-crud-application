data.json
{"title":"event 11","body":"body for event 11"}

curl --request PUT  --url https://udemy-utils.herokuapp.com/api/v1/events/11?token=token123 --header "Content-Type: application/json" --data @data2.json

data2.json
{"title":"event 11u","body":"body for event 11u"}


curl --request DELETE  --url https://udemy-utils.herokuapp.com/api/v1/events/12?token=token123